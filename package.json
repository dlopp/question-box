{
  "name": "question-box",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier . --check --ignore-path .prettierignore",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:style",
    "format": "yarn lint:eslint --fix && yarn lint:prettier --write && yarn lint:style --fix",
    "heroku-postbuild": "yarn build",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/firebase": "^7.5.0",
    "cookieparser": "^0.1.0",
    "core-js": "^3.8.2",
    "firebase": "^8.3.0",
    "js-cookie": "^2.2.1",
    "nuxt": "^2.14.12"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.4.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.3",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "standard-version": "^9.2.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  }
}
